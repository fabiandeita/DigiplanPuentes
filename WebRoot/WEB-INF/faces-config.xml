<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>navigation</managed-bean-name>
  <managed-bean-class>bean.NavigationBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>treeNavigation</managed-bean-name>
  <managed-bean-class>bean.TreeNavigation</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>content</managed-bean-name>
  <managed-bean-class>bean.PageContentBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>logeo</managed-bean-name>
  <managed-bean-class>bean.Logeo</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>conf</managed-bean-name>
  <managed-bean-class>conf.Configuration</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>media</managed-bean-name>
  <managed-bean-class>conf.Media</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>catalogo</managed-bean-name>
  <managed-bean-class>bean.Catalogo</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>captacion</managed-bean-name>
  <managed-bean-class>bean.Captacion</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>busqueda</managed-bean-name>
  <managed-bean-class>bean.Busqueda</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>volcar</managed-bean-name>
  <managed-bean-class>bean.Volcar</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>exportar</managed-bean-name>
  <managed-bean-class>bean.Exportar</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/mainTemplate.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/navigation.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/content-facelet.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/country-facelet.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Busqueda.jspx</from-view-id>
  <navigation-case>
   <from-outcome>toMenu</from-outcome>
   <to-view-id>/MenuPrincipal.jspx</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>toDetalle</from-outcome>
   <to-view-id>/Detalle.jspx</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>toBusqueda</from-outcome>
   <to-view-id>/Busqueda.jspx</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/header.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/headerHtml.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/footer.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Logeo.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/MenuPrincipal.jspx</from-view-id>
  <navigation-case>
   <from-outcome>toCapture</from-outcome>
   <to-view-id>/Captacion.jspx</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>toBusqueda</from-outcome>
   <to-view-id>/Busqueda.jspx</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>toCatalogos</from-outcome>
   <to-view-id>/Catalogos.jspx</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Captacion.jspx</from-view-id>
  <navigation-case>
   <from-outcome>toMenu</from-outcome>
   <to-view-id>/MenuPrincipal.jspx</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Detalle.jspx</from-view-id>
  <navigation-case>
   <from-outcome>toBusqueda</from-outcome>
   <to-view-id>/Busqueda.jspx</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Catalogos.jspx</from-view-id>
  <navigation-case>
   <from-outcome>toMenu</from-outcome>
   <to-view-id>/MenuPrincipal.jspx</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Importacion.jspx</from-view-id>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Exportar.jspx</from-view-id>
 </navigation-rule>
 <application>
  <view-handler>com.icesoft.faces.facelets.D2DFaceletViewHandler</view-handler>
 </application>
</faces-config>
