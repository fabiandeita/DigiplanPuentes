							</ice:commandLink>
					
						<!--</ice:panelGrid> -->
					
		    		</ice:panelGrid>
	    		 </ice:panelCollapsible>
	    		<ice:panelCollapsible id="linkPanel" expanded="#{busqueda.panelCollapsible}"  actionListener="#{busqueda.changePanelCollapsible}" >
